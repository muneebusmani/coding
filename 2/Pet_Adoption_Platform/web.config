<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Routing">
                    <match url="^(?!.*assets\/)(.+)$"/>
                    <conditions>
                        <add input="{REQUEST_FILENAME}" pattern="^(.+)$" />
                    </conditions>
                    <action type="Rewrite" url="index.php?url={R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
